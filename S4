//Crear una funci贸n que reciba un array y una cadena. La funci贸n debe revisar si la cadena ya se encuentra en el array y
//si no agregarla al final y siempre debe devolver la posici贸n del array donde se encuentra ya sea que existiera o si la agreg贸.

let array = ['a', 'b', 'c', 'e'];
let cadena = 'asas';
let i;

i = array.findIndex((element) => element == cadena);
if (-1 == i) {
  array.push(cadena);
  i = array.length - 1;
}

console.log(array);
console.log(i);
